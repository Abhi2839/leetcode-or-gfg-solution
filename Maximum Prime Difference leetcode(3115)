class Solution {
public:
bool check (int a){
    if (2>a) return false;
    for (int i=2;a>=i*i;i++){
        if (a%i==0) return false;
    }
    return true;
}
    int maximumPrimeDifference(vector<int>& nums) {
        int f=-1,l=-1;
        for (int i=0;nums.size()>i;i++){
            if (check(nums[i]) and f==-1){
                f=i;
            }
            if(f!=-1 and check(nums[i]))
            l=i;
        }
        return abs(f-l);
    }
};
